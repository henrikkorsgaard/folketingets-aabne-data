{{define "lovforslag"}}

    <h1>{{.Titel}}</h1>
    <h2>{{.Lovnummer}}</h2>
    <p>{{.Resume}}</p>

{{end}}

{{define "list"}}
    
    {{range .}}
    <h1><a href="/lovforslag/{{.Id}}">{{.Lovnummer}}: {{.TitelKort}}</a></h1>
    {{end}}

{{end}}

{{define "update"}} 

    <h1>Updated database successfully</h1>
    <ul>
        <li>Inserted {{.Count}} new Lovforslag</li>
        <li>Database contains {{.Total}} Lovforslag</li>
    </ul>
{{end}}